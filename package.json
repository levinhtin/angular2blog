{
  "name": "angular2blog",
  "version": "2.0.0",
  "description": "Blog using angular 2",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "build": "gulp dev",
    "lite": "lite-server --baseDir ./src --port 5000",
    "start": "concurrent \"npm run tsc:w\" \"npm run lite\" ",
    "lint": "gulp tslint",
    "testonweb": "live-server --open=src/unit-test.html --port=3000",
    "test": "concurrent \"npm run tsc\" \"karma start karma.conf.js\" ",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "author": "levinhtin",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "angular2-in-memory-web-api": "0.0.7",
    "bootstrap": "^3.3.6",
    "es6-shim": "^0.35.0",
    "jquery": "^2.2.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "^0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^1.0.0",
    "del": "^2.2.0",
    "eslint": "2.4.0",
    "gulp": "^3.9.0",
    "gulp-inject": "^3.0.0",
    "gulp-tslint": "^4.3.3",
    "jasmine": "^2.4.1",
    "jasmine-core": "2.4.1",
    "karma": "0.13.19",
    "karma-chrome-launcher": "0.2.2",
    "karma-coverage": "0.5.3",
    "karma-jasmine": "0.3.6",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "lite-server": "^1.3.4",
    "live-server": "0.8.1",
    "mocha": "^2.4.5",
    "phantomjs-polyfill": "0.0.1",
    "phantomjs-prebuilt": "^2.1.4",
    "remap-istanbul": "0.5.1",
    "tslint": "^3.5.0",
    "typescript": "^1.8.10",
    "typings": "0.7.9"
  }
